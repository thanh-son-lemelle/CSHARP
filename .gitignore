Thumbs.db
Thumbs.db.meta

# Thumbnail cache files
*.stackdump

# Files for iOS and Android
*.apk
*.aab
*.ipa
*.xcuserstate

# Ignore the gradle build folder in Android builds
.gradle/

# Ignore the Unity Asset Store tools directory
Assets/AssetStoreTools*

# Logs, builds, and local project files created by Unity.
Logs/
MemoryCaptures/
*.pidb.meta
*.pidb

# Visual Studio Code
.vscode/

# Rider
.idea/
*.sln.iml

# Unity3D generated file on crash reports
sysinfo.txt

# Gradle cache (used in Android builds)
.gradle/

# NuGet Packages
*.nupkg
# NuGet Symbol Packages
*.snupkg

# Ignore the Unity generated meta files
*.pidb
*.pidb.meta
